{
  "name": "@formal-language/grammar",
  "description": "Grammar compilation toolkit for JavaScript",
  "version": "8.0.0",
  "author": "make-github-pseudonymous-again",
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "3m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/formal-language/grammar/issues"
  },
  "dependencies": {
    "@iterable-iterator/chain": "^2.0.1",
    "@iterable-iterator/filter": "^1.0.1",
    "@iterable-iterator/iter": "^1.0.1",
    "@iterable-iterator/list": "^1.0.1",
    "@iterable-iterator/map": "^1.0.1",
    "@iterable-iterator/next": "^2.0.1",
    "@iterable-iterator/reduce": "^1.0.1",
    "@iterable-iterator/reversed": "^1.0.0",
    "@iterable-iterator/zip": "^1.0.1"
  },
  "devDependencies": {
    "@async-abstraction/tape": "^8.0.0",
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-async-generator-functions": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/register": "7.16.0",
    "@iterable-iterator/range": "^2.1.0",
    "@iterable-iterator/repeat": "^1.0.1",
    "@iterable-iterator/sorted": "^1.0.0",
    "@total-order/primitive": "^1.0.1",
    "ava": "3.15.0",
    "coveralls": "3.1.1",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "7.5.0",
    "nyc": "15.1.0",
    "regenerator-runtime": "0.13.9"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://formal-language.github.io/grammar",
  "keywords": [
    "compilation",
    "grammar",
    "language"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/formal-language/grammar"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  },
  "sideEffects": false
}

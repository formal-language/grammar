{
  "name": "@aureooms/js-grammar",
  "description": "Grammar compilation toolkit for JavaScript",
  "version": "8.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "3m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-grammar/issues"
  },
  "dependencies": {
    "@aureooms/js-itertools": "^4.0.1"
  },
  "devDependencies": {
    "@aureooms/js-compare": "1.4.5",
    "@aureooms/js-tape": "7.0.0",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/plugin-proposal-async-generator-functions": "7.10.5",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/register": "7.10.5",
    "ava": "3.11.1",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.3.2",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-grammar",
  "keywords": [
    "compilation",
    "grammar",
    "language"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-grammar"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
